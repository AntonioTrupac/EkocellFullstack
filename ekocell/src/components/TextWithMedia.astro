---
import type {
  FormattedElement,
  FormattedText,
} from "../../../payload/src/rich-text-export";
import RichText from "./RichText.astro";

type Props = {
  markdown: (FormattedElement | FormattedText)[];
};

const { markdown } = Astro.props;
---

